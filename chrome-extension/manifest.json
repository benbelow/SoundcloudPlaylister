{
  "manifest_version": 2,

  "name": "Soundcloud Playlister",
  "description": "This extension will make a Soundcloud playlist",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
 ],
   "content_scripts" : [{
        "matches" : [ "www.reddit.com/*"],
        "js" : ["https://connect.soundcloud.com/sdk/sdk-3.1.2.js"]
    }]
}
